# Mock API Service Configuration
PORT=3001
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=info

# API Configuration
API_VERSION=v1
API_PREFIX=/api

# Mock Data Configuration
MOCK_DELAY_MIN=100
MOCK_DELAY_MAX=500
MOCK_ERROR_RATE=0.1

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 